                android:textColor="@color/black"
                android:textSize="@dimen/large_text_size"
                app:layout_constraintStart_toEndOf="@+id/large_shapeableImageView"
                app:layout_constraintTop_toTopOf="@+id/large_shapeableImageView" />

            <com.google.android.material.imageview.ShapeableImageView
                android:id="@+id/large_shapeableImageView"
                android:layout_width="80dp"
                android:layout_height="80dp"
                android:layout_marginStart="@dimen/size_normal"
                android:layout_marginTop="@dimen/size_normal"
                android:elevation="@dimen/normal_elevation"
                android:src="@drawable/someone"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:shapeAppearanceOverlay="@style/circle"
                tools:ignore="MissingConstraints" />

            <TextView
                android:id="@+id/massage_text"
                style="@style/textView_call_massage_more"
                android:text="@string/massage_string"
                app:layout_constraintEnd_toStartOf="@+id/more_text"
                app:layout_constraintStart_toEndOf="@+id/call_text"
                app:layout_constraintTop_toBottomOf="@+id/frameMassangerIcon" />

            <TextView
                android:id="@+id/more_text"
                style="@style/textView_call_massage_more"
                android:text="@string/more_string"
                app:layout_constraintEnd_toEndOf="@+id/pearl_Smith"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toEndOf="@+id/massage_text"
                app:layout_constraintTop_toBottomOf="@+id/frameMoreIcon" />

            <TextView
                android:id="@+id/call_text"
                style="@style/textView_call_massage_more"
                android:text="@string/call"
                app:layout_constraintEnd_toStartOf="@+id/massage_text"
                app:layout_constraintHorizontal_bias="0.5"
                app:layout_constraintStart_toStartOf="@+id/pearl_Smith"
                app:layout_constraintTop_toBottomOf="@+id/frameCallIcon" />


        </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.constraintlayout.motion.widget.MotionLayout>

